---
import LayoutForGames from "@layouts/LayoutForGames.astro";
---

<LayoutForGames>
  <div id="society-multiphobics-root"></div>
</LayoutForGames>

<script>
  import SocietyOfMultiphobics from '@all-games/marketplace/the-society-of-multiphobics';
  import React from 'react';
  import { createRoot } from 'react-dom/client';

  document.addEventListener('DOMContentLoaded', () => {
    const container = document.getElementById('society-multiphobics-root');
    if (container) {
      const root = createRoot(container);
      root.render(React.createElement(SocietyOfMultiphobics));
    }
  });
</script>

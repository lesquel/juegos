{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": ".*"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
